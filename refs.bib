@article{Lee04,
  abstract =     {A conceptual framework for integrating diverse functional
                  genomics data was developed by reinterpreting experiments to
                  provide numerical likelihoods that genes are functionally
                  linked. This allows direct comparison and integration of
                  different classes of data. The resulting probabilistic gene
                  network estimates the functional coupling between genes.
